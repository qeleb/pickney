{
  "name": "pickney",
  "version": "0.1.2",
  "private": true,
  "author": "Caleb Hoff & Ramin Nourbakhsh",
  "main": "index.js",
  "scripts": {
    "build": "npx webpack -p",
    "deploy": "npm run build && git commit -a -m deployment && git push heroku master",
    "dev": "webpack-dev-server --open",
    "server": "npx cross-env NODE_ENV=production babel-node src/server",
    "server-dev": "npx nodemon src/server --exec babel-node src/server",
    "start-dev": "npx concurrently \"npm run server-dev\" \"npm run dev\"",
    "start": "npm run server"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.1.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "history": "^5.1.0",
    "md5": "^2.3.0",
    "mongodb": "^4.10.0",
    "multer": "^1.4.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-paginate": "^8.0.3",
    "react-redux": "^5.1.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/register": "^7.16.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "nodemon": "^2.0.15",
    "style-loader": "^2.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  }
}
